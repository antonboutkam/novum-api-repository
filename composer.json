{
    "name": "novum/api-repository",
    "description": "Repository API, contains a collection of installable packages for the Novum Innovation App.",
    "type": "novum-api",
    "require": {
        "novum/innovation-app-core": "*",
        "novum/api-domain": "dev-master",
        "hurah/hurah-installer": "*",
        "PHP": ">=7.4"
    },
    "license": "MIT",
    "prefer-stable": false,
    "homepage": "https://docs.demo.novum.nu/",
    "minimum-stability": "dev",
    "keywords": [
        "api", "innovation app", "api files"
    ],
    "authors": [
        {
            "name": "Anton",
            "email": "anton@novum.nu"
        }
    ],
    "extra": {
        "install_dir": "api.repository.demo.novum.nu"
    },
    "autoload": {
        "psr-4": {
            "ApiNovumRepository\\": "modules"
        }
    }
}
